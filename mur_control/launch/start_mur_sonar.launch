<launch>
  <!-- SONAR -->
  <node pkg="tf2_ros" type="static_transform_publisher" name="robot_broadcaster_sonar" args="0.0 0.21 0.057 0 0 0.707 0.707 /mur/base_link /mur/sonar" />
  <include file="$(find tritech_micron)/launch/tritech_micron.launch">
    <arg name="port" value="/dev/ttyUSB0"/>
    <arg name="frame" value="/mur/sonar"/>
  </include>
  <node pkg="mur_control" type="mur_sonar_converter.py" name="mur_sonar_converter" output="screen"></node>
</launch>
